{
  "name": "sports-center-backend",
  "version": "1.0.0",
  "bin": "dist/server.js",
  "pkg": {
    "scripts": "dist/**/*.js",
    "assets": ["src/**/*.ts", ".env"],
    "targets": ["node20-win-x64"]
  },
  "scripts": {
    "build": "tsc",
    "prebuild": "rimraf dist",
    "dev": "ts-node-dev src/server.ts",
    "start": "node dist/server.js",
    "prepkg": "npm run build",
    "pkg": "pkg . --no-bytecode --public --compress GZip"
  },
  "dependencies": {
    "@types/multer": "^2.0.0",
    "ali-oss": "^6.23.0",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^2.0.2",
    "mysql2": "^3.6.1"
  },
  "devDependencies": {
    "@types/ali-oss": "^6.16.11",
    "@types/bcrypt": "^5.0.1",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.11.19",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3",
    "uuid": "^11.1.0"
  }
}
